{
  "name": "scss-bem-highlight",
  "displayName": "SCSS BEM Highlight",
  "description": "Plugin that provides syntax highlighting for BEM notation in SCSS files",
  "version": "0.1.0",
  "publisher": "PavelCherdantsev",
  "icon": "assets/bem.png",
  "engines": { "vscode": "^1.33.0" },
  "categories": ["Other"],
  "repository": { "url": "https://github.com/chepash/scss-bem-support" },
  "contributes": {
    "grammars": [{
      "injectTo": ["source.css.scss"],
      "scopeName": "bem.support.scss",
      "path": "./syntaxes/bemSupport.tmLanguage.json"
    }]
  },
  "author": "joloyonaha",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && mkdir build && vsce package --out build/"
  }
}
